{
  "name": "marimekko",
  "version": "1.0.0",
  "description": "",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "build:dev": "docker compose -f docker-compose.development.yml --env-file .env.development build",
    "up:dev": "docker compose -f docker-compose.development.yml --env-file .env.development up -d",
    "migrate:create": "docker exec marimekko-app npm run payload migrate:create"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/storage-gcs": "latest",
    "@radix-ui/themes": "latest",
    "@svgr/webpack": "^8.1.0",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "next": "15.0.4",
    "payload": "latest",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "22.12.0",
    "npm": "10.9.0"
  },
  "packageManager": "npm@10.9.0"
}
